			<id>central</id>
			<url>http://repo1.maven.org/maven2/</url>
			<name>maven2</name>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>

